{
  "name": "autogestion-web",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev-publish": "ng build --configuration=dev-publish && npm run build:obfuscate && xcopy .htaccess \"solicitudcreditoenlinea\" /F /Y",
    "test-publish": "ng build --configuration=test-publish && npm run build:obfuscate && xcopy .htaccess \"solicitudcreditoenlinea\" /F /Y",
    "prod-publish": "ng build --configuration=prod-publish && npm run build:obfuscate && xcopy .htaccess \"solicitudcreditoenlinea\" /F /Y",
    "build:obfuscate": "javascript-obfuscator ./solicitudcreditoenlinea --output ./ofuscado && xcopy \"./ofuscado/solicitudcreditoenlinea\" \"solicitudcreditoenlinea\" /F /Y /S && npm run build:delete",
    "build:delete": "rmdir /S /Q ofuscado",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
